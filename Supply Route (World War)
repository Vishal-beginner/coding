..//supply route-world war


#define cheat <bits/stdc++.h>
#include cheat
#define ll long long
#define _CRT_SECURE_NO_WARNINGS
using namespace std;
ll a[100][100];
ll dp[100][100];
 
int main(int argc, char** argv)
{
    int test_case;
    int T;
    /*
    The freopen function below opens input.txt in read only mode and
    sets your standard input to work with the opened file.
    When you test your code with the sample data, you can use the function
    below to read in from the sample data file instead of the standard input.
    So. you can uncomment the following line for your local test. But you
    have to comment the following line when you submit for your scores.
    */
 
    cin >> T;
 
    /*
    Read each test case from standard input.
    */
 
    for (test_case = 1; test_case <= T; ++test_case)
    {
        ll n;
        cin >> n;
        for (int i = 0; i < n; i++)
        {
            string s;
            cin >> s;
            for (int j = 0; j < n; j++)
            {
                a[i][j] = s[j] - '0';
                dp[i][j] = INT_MAX;
            }
        }
        dp[0][0] = 0;
        int x[] = { 1, -1, 0, 0 };
        int y[] = { 0, 0, 1, -1 };
 
        set < pair<ll, pair<ll, ll>>>s;
        s.insert({ 0, { 0, 0 } });
        while (!s.empty())
        {
            pair<int, pair<int, int>>p = *s.begin();
            s.erase(s.begin());
            ll x1 = p.second.first;
            ll y1 = p.second.second;
            ll dis = p.first;
            for (int i = 0; i < 4; i++)
            {
                if (x1 + x[i] < n&&x1 + x[i] >= 0 && y1 + y[i] < n&&y1 + y[i] >= 0)
                {
                    if (dp[x1 + x[i]][y1 + y[i]]>a[x1][y1] + dis)
                    {
                        if (dp[x1 + x[i]][y1 + y[i]] != INT_MAX)
                        {
                            s.erase(s.find({ dp[x1 + x[i]][y1 + y[i]], { x1 + x[i], y1 + y[i] } }));
                        }
                        dp[x1 + x[i]][y1 + y[i]] = a[x1][y1] + dis;
                        s.insert({ dp[x1 + x[i]][y1 + y[i]], { x1 + x[i], y1 + y[i] } });
                    }
                }
            }
        }
         
        cout << "#" << test_case << " " <<dp[n-1][n-1]<< endl;
 
    }
    return 0;//Your program should return 0 on normal termination.
}
