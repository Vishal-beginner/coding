#include<iostream>
#include<set>
#include<vector>
 
#include<string>
#include<algorithm>
 
using namespace std;
 
 
void a007k(int t)
{
    int m;
    cin >> m;
 
    string s;
    cin >> s;
 
    set<string>st;
 
    int n = s.size();
    int size=0;
    for (int i = 0; i < n; i++)
    {
        for (int j = 0; j <= i; j++)
        {
            string ans = "";
            for (int k = j; k <= i; k++)
                ans += s[k];
            st.insert(ans);
        }
    }
     
     
     
    string ans;
   // sort(v.begin(), v.end());
    if (m > st.size())
        ans = "none";
    else{
           std::set<string>::iterator it;
           it=st.begin();
           advance(it,m-1);
           ans=*it;
        }
    //for (int i = 0; i < v.size(); i++)
        //cout << v[i] << endl;;
 
    cout <<"#"<<t<<" "<< ans << endl;
}
 
 
int main()
{
    int test;
    cin >> test;
 
    for (int i = 1; i <= test; i++)
    {
        a007k(i);
    }
 
    //system("PAUSE");
 
}
