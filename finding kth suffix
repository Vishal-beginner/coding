#include<iostream>
#include<vector>
#include<string>
#include<algorithm>
 
using namespace std;
 
void a007k(int t)
{
    int n;
    cin >> n;
 
    string s;
    cin >> s;
 
    vector<string>v;
 
 
    for (int i = 0; i < s.size(); i++)
    {
        v.push_back(s.substr(i,s.size()));
    }
 
    sort(v.begin(), v.end());
    //for (int i = 0; i < v.size(); i++)
        //cout << v[i] << endl;;
 
    cout <<"#"<<t<<" "<< v[n-1] << endl;
}
 
 
int main()
{
    int test;
    cin >> test;
 
    for (int i = 1; i <= test; i++)
    {
        a007k(i);
    }
 
    //system("PAUSE");
 
}
