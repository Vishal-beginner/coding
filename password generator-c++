#include<iostream>
 
using namespace std;
 
 
void a007k(int t)
{
    int k;
    cin >> k;
    int arr[8];
    for (int i = 0; i < 8; i++)
    {
        cin >> arr[i];
    }
 
    int n = arr[0];
    int g = 0;
    while (n > 0)
    {
        g++;
        arr[0] =arr[0]-g;
        n = arr[0];
        if (arr[0] < 0)
            arr[0] = 0;
        int temp = arr[0];
        for (int i = 0; i < 7; i++)
        {
            arr[i] = arr[i + 1];
        }
        arr[7] = temp;
 
        //rotrte;
 
        if (g == 5)
            g = 0;
 
    }
    cout<<"#"<<t<<" ";
    for (int i = 0; i < 8; i++)
    {
        cout << arr[i] << " ";
    }
 
   cout<<endl;
}
 
int main()
{
    for (int i = 1; i <= 10; i++)
    {
        a007k(i);
    }
    //system("PAUSE");
}
